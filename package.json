  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caracoljs/lock.git"
  },
  "private": false,
  "dependencies": {
    "commander": "^7.2.0",
    "futoin-hkdf": "^1.3.3",
    "keytar": "^7.2.0",
